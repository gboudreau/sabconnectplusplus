{
	"name": "SABconnect++",
	"version": "0.5.9",
	"manifest_version": 2,
	"description": "SABnzbd extension for Google Chrome.",
	"background": {
        "page": "background.html"
	},
	"options_page": "settings.html",
	"browser_action": {
		"default_icon": "images/sab2_16.png",
		"default_title": "SABnzbd",
		"default_popup": "popup.html"
	},
	"icons":
	{
		"16": "images/sab2_16.png",
		"48": "images/sab2_48.png",
		"128": "images/sab2_128.png"
	},
	"permissions":
	[
		"http://*/*",
		"https://*/*",
		"tabs",
		"notifications",
		"contextMenus",
		"background"
	],
	"web_accessible_resources": [
	    "images/sab2_16.png",
	    "images/sab2_48.png",
	    "images/sab2_16_red.png",
	    "images/sab2_16_green.png",
	    "images/sab2_16_fetching.png",
	    "images/sab2_16_blue.png",
	    "images/nzbmatrix_button.gif",
	    "images/messagebox_critical.png",
	    "images/control_play.png",
	    "images/control_pause.png"
	],
	"content_scripts": [
		{
			"matches": [
				"http://*.newzbin2.es/*",
				"https://*.newzbin2.es/*",
				"http://*.newzxxx.com/*",
				"https://*.newzxxx.com/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"scripts/content/newzbin.js"
			]
		},
		{
			"matches": [
				"http://*.nzbmatrix.com/",
				"http://*.nzbmatrix.com/*",
				"https://*.nzbmatrix.com/",
				"https://*.nzbmatrix.com/*",
				"http://*.nzbxxx.com/",
				"http://*.nzbxxx.com/*",
				"https://*.nzbxxx.com/",
				"https://*.nzbxxx.com/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"scripts/content/nzbmatrix.js"
			],
			"css": ["css/nzbmatrix.css"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzbclub.com/",
				"http://*.nzbclub.com/*",
				"https://*.nzbclub.com/",
				"https://*.nzbclub.com/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"scripts/content/nzbclub.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.bintube.com/",
				"http://*.bintube.com/*",
				"https://*.bintube.com/",
				"https://*.bintube.com/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"scripts/content/bintube.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.binsearch.info/",
				"http://*.binsearch.info/*",
				"https://*.binsearch.info/",
				"https://*.binsearch.info/*",
				"http://binsearch.info/",
				"http://binsearch.info/*",
				"https://binsearch.info/",
				"https://binsearch.info/*",
				"http://*.binsearch.net/",
				"http://*.binsearch.net/*",
				"https://*.binsearch.net/",
				"https://*.binsearch.net/*",
				"http://binsearch.net/",
				"http://binsearch.net/*",
				"https://binsearch.net/",
 				"https://binsearch.net/*",
 				"http://*.binsearch.co.uk/",
 				"http://*.binsearch.co.uk/*",
 				"https://*.binsearch.co.uk/",
 				"https://*.binsearch.co.uk/*",
 				"http://binsearch.co.uk/",
 				"http://binsearch.co.uk/*",
 				"https://binsearch.co.uk/",
 				"https://binsearch.co.uk/*",
 				"http://*.binsear.ch/",
 				"http://*.binsear.ch/*",
 				"https://*.binsear.ch/",
 				"https://*.binsear.ch/*",
 				"http://binsear.ch/",
 				"http://binsear.ch/*",
 				"https://binsear.ch/",
 				"https://binsear.ch/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"scripts/content/binsearch.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzbindex.com/",
				"http://*.nzbindex.com/*",
				"https://*.nzbindex.com/",
				"https://*.nzbindex.com/*",
				"http://nzbindex.com/",
				"http://nzbindex.com/*",
				"https://nzbindex.com/",
				"https://nzbindex.com/*",
				"http://*.nzbindex.nl/",
				"http://*.nzbindex.nl/*",
				"https://*.nzbindex.nl/",
				"https://*.nzbindex.nl/*",
				"http://nzbindex.nl/",
				"http://nzbindex.nl/*",
				"https://nzbindex.nl/",
				"https://nzbindex.nl/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"scripts/content/nzbindex.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzbsrus.com/",
				"http://*.nzbsrus.com/*",
				"https://*.nzbsrus.com/",
				"https://*.nzbsrus.com/*",
				"http://nzbsrus.com/",
				"http://nzbsrus.com/*",
				"https://nzbsrus.com/",
				"https://nzbsrus.com/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"third_party/webtoolkit/webtoolkit.base64.js",
				"scripts/content/nzbsrus.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzb.su/",
				"http://*.nzb.su/*",
				"http://nzb.su/",
				"http://nzb.su/*",
				"https://*.nzb.su/",
				"https://*.nzb.su/*",
				"https://nzb.su/",
				"https://nzb.su/*",
				"http://*.nzbs.org/",
				"http://*.nzbs.org/*",
				"https://*.nzbs.org/",
				"https://*.nzbs.org/*",
				"http://*.nzbhq.com/",
				"http://*.nzbhq.com/*",
				"https://*.nzbhq.com/",
				"https://*.nzbhq.com/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"third_party/webtoolkit/webtoolkit.base64.js",
				"scripts/content/newznab.js"
			],
			"css": ["css/newznab.css"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.fanzub.com/",
				"http://*.fanzub.com/*",
				"http://fanzub.com/",
				"http://fanzub.com/*",
				"https://*.fanzub.com/",
				"https://*.fanzub.com/*",
				"https://fanzub.com/",
				"https://fanzub.com/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"third_party/webtoolkit/webtoolkit.base64.js",
				"scripts/content/fanzub.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.dognzb.cr/",
				"http://*.dognzb.cr/*",
				"http://dognzb.cr/",
				"http://dognzb.cr/*",
				"https://*.dognzb.cr/",
				"https://*.dognzb.cr/*",
				"https://dognzb.cr/",
				"https://dognzb.cr/*"
			],
			"js": [
				"third_party/jquery/jquery-1.7.2.min.js",
				"scripts/content/common.js",
				"third_party/webtoolkit/webtoolkit.base64.js",
				"scripts/content/dognzb.js"
			],
			"all_frames": true
		}
	]
}
