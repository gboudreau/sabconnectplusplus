{
	"name": "SABconnect++",
	"version": "0.5.6",
	"description": "SABnzbd extension for Google Chrome.",
	"background_page": "background.html",
	"options_page": "settings.html",
	"browser_action": {
		"default_icon": "images/sab2_16.png",
		"default_title": "SABnzbd",
		"popup": {
			"height": 200,
			"path": "popup.html"
		}
	},
	"icons":
	{
		"16": "images/sab2_16.png",
		"48": "images/sab2_48.png",
		"128": "images/sab2_128.png"
	},
	"permissions":
	[
		"http://*/*",
		"https://*/*",
		"tabs",
		"notifications",
		"contextMenus"
	],
	"content_scripts": [
		{
			"matches": [
				"http://*.newzbin.com/*",
				"https://*.newzbin.com/*",
				"http://*.newzxxx.com/*",
				"https://*.newzxxx.com/*"
			],
			"js": [
				"third_party/jquery/jquery-1.4.2.min.js",
				"scripts/bind.js",
				"scripts/content/common.js",
				"scripts/content/newzbin.js"
			]
		},
		{
			"matches": [
				"http://*.nzbmatrix.com/",
				"http://*.nzbmatrix.com/*",
				"https://*.nzbmatrix.com/",
				"https://*.nzbmatrix.com/*",
				"http://*.nzbxxx.com/",
				"http://*.nzbxxx.com/*",
				"https://*.nzbxxx.com/",
				"https://*.nzbxxx.com/*"
			],
