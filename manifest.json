				"scripts/bind.js",
				"scripts/content/common.js",
				"scripts/content/nzbmatrix.js"
			],
			"css": ["css/nzbmatrix.css"],
			"all_frames": true
		},
		{
			"matches": [
				"http://*.nzbclub.com/",
				"http://*.nzbclub.com/*",
				"https://*.nzbclub.com/",
				"https://*.nzbclub.com/*"
			],
